<ul>
    <li> <%= link_to "Events", events_path %> </li>
    <li> <%= link_to "Users", users_path %> </li>
    <% if !logged_in? %>
        <li> <%= link_to "Login", login_path, :method => :get %> </li>
        <li><%= link_to "Create Account", new_user_url, :method => :get %></li>
    <% else %>
        <li> <%= link_to "Logout", login_path, :method => :delete %> </li>
        <li> <%= link_to "Create Event", new_event_url, :method => :get %> </li>
        <li> <%= link_to "Home", user_path(session[:user_id]) %> </li>
    <% end %>
</ul>